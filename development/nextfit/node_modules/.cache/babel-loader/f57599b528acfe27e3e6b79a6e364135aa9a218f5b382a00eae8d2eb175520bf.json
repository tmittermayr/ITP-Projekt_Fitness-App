  mounted() {\n    this.renderChart();\n  },\n  methods: {\n    renderChart() {\n      const data = this.muscleData.map((item) => ({\n        name: item.name,\n        y: item.percentage\n      })); // Konvertiere das Array in das erforderliche Datenformat\n\n      Highcharts.chart('chart', {\n        chart: {\n          type: 'pie'\n        },\n        title: {\n          text: 'Trainierte Muskelgruppen'\n        },\n        tooltip: {\n          pointFormat: '{series.name}: <b>{point.percentage:.1f}%</b>'\n        },\n        series: [\n          {\n            name: 'Anteil',\n            colorByPoint: true,\n            data: data\n          }\n        ]\n      });\n    },\n    async getData() {\n      axios.defaults.headers.common['Authorization'] = `Bearer ${await this.getToken()}`;\n      axios\n        .get('http://localhost:3000/stats/total')\n        .then(function (response) {\n          console.log(response) \n          \n        })\n    },\n    async  getToken() {\n        const { value } = await Preferences.get({ key: 'token' })\n        return value ? value : ''\n    }\n  }\n};\n</script>\n\n<style>\n.chart {\n  width: 100%;\n  height: 400px;\n\n}\n\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}